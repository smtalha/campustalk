    <Grid Background="#f15a29">
        <Grid VerticalAlignment="Center"
              HorizontalAlignment="Center"
              Width="1366"
              Height="768">
            <Grid.RowDefinitions>
                <RowDefinition Height="284" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>

            <TextBlock x:Name="textBlock"
                       Text="Sign in"
                       FontSize="48"
                       Foreground="White"
                       Grid.Row="1"
                       HorizontalAlignment="Center"
                       Margin="0,5,0,0"
                       FontFamily="Segoe UI"
                       FontWeight="Light"
                       Opacity="0"
                       RenderTransformOrigin="0.5,0.5">
                <TextBlock.RenderTransform>
                    <CompositeTransform />
                </TextBlock.RenderTransform>
            </TextBlock>

            <TextBox x:Name="signUpName"
                     HorizontalAlignment="Center"
                     Margin="48,93,0,0"
                     Grid.Row="1"
                     TextWrapping="Wrap"
                     VerticalAlignment="Top"
                     Width="300"
                     Height="40"
                     FontFamily="Segoe UI"
                     FontWeight="Light"
                     FontSize="21"
                     PlaceholderText="Name"
                     SelectionHighlightColor="#FFF15A29"
                     Opacity="0"
                     RenderTransformOrigin="0.5,0.5">
                <TextBox.RenderTransform>
                    <CompositeTransform />
                </TextBox.RenderTransform>
                <!-- Flyout-->
                <FlyoutBase.AttachedFlyout>
                    <Flyout Placement="Bottom">
                        <Flyout.FlyoutPresenterStyle>
                            <Style TargetType="FlyoutPresenter">
                                <Setter Property="BorderThickness"
                                        Value="3" />
                                <Setter Property="BorderBrush"
                                        Value="#f15a29" />
                            </Style>
                        </Flyout.FlyoutPresenterStyle>
                        <Grid>
                            <!-- Text-->
                            <TextBlock Text="Name should contain at least 2 characters"
                                       FontFamily="Segoe UI"
                                       FontSize="16"
                                       FontWeight="Medium"
                                       Foreground="Black" />
                            <!-- Text END-->
                        </Grid>
                    </Flyout>
                </FlyoutBase.AttachedFlyout>
                <!-- Flyout END-->
            </TextBox>
            <TextBox x:Name="signUpEmail"
                     HorizontalAlignment="Center"
                     Margin="48,157,0,0"
                     Grid.Row="1"
                     TextWrapping="Wrap"
                     VerticalAlignment="Top"
                     Width="300"
                     Height="40"
                     FontFamily="Segoe UI"
                     FontWeight="Light"
                     FontSize="21"
                     PlaceholderText="Email"
                     InputScope="EmailSmtpAddress"
                     SelectionHighlightColor="#FFF15A29"
                     Opacity="0"
                     RenderTransformOrigin="0.5,0.5">
                <TextBox.RenderTransform>
                    <CompositeTransform />
                </TextBox.RenderTransform>
                <!-- Flyout-->
                <FlyoutBase.AttachedFlyout>
                    <Flyout Placement="Bottom">
                        <Flyout.FlyoutPresenterStyle>
                            <Style TargetType="FlyoutPresenter">
                                <Setter Property="BorderThickness"
                                        Value="3" />
                                <Setter Property="BorderBrush"
                                        Value="#f15a29" />
                            </Style>
                        </Flyout.FlyoutPresenterStyle>
                        <Grid>
                            <!-- Text-->
                            <TextBlock Text="Please enter a valid email address."
                                       FontFamily="Segoe UI"
                                       FontSize="16"
                                       FontWeight="Medium"
                                       Foreground="Black" />
                            <!-- Text END-->
                        </Grid>
                    </Flyout>
                </FlyoutBase.AttachedFlyout>
                <!-- Flyout END-->
            </TextBox>
            <TextBox x:Name="signUpUsername"
                     HorizontalAlignment="Center"
                     Margin="48,220,0,0"
                     Grid.Row="1"
                     TextWrapping="Wrap"
                     VerticalAlignment="Top"
                     Width="300"
                     Height="40"
                     FontFamily="Segoe UI"
                     FontWeight="Light"
                     FontSize="21"
                     PlaceholderText="Username"
                     SelectionHighlightColor="#FFF15A29"
                     Opacity="0"
                     RenderTransformOrigin="0.5,0.5">
                <TextBox.RenderTransform>
                    <CompositeTransform />
                </TextBox.RenderTransform>
                <!-- Flyout-->
                <FlyoutBase.AttachedFlyout>
                    <Flyout Placement="Bottom">
                        <Flyout.FlyoutPresenterStyle>
                            <Style TargetType="FlyoutPresenter">
                                <Setter Property="BorderThickness"
                                        Value="3" />
                                <Setter Property="BorderBrush"
                                        Value="#f15a29" />
                            </Style>
                        </Flyout.FlyoutPresenterStyle>
                        <Grid Width="550"
                              Grid.ColumnSpan="2">
                            <!-- Text-->
                            <TextBlock Text="Username can contain lowercase letters, numbers and periods."
                                       FontFamily="Segoe UI"
                                       FontSize="16"
                                       FontWeight="Medium"
                                       Foreground="Black" />
                            <!-- Text END-->
                        </Grid>
                    </Flyout>
                </FlyoutBase.AttachedFlyout>
                <!-- Flyout END-->
            </TextBox>

            <Button x:Name="signUpButton"
                    Content="Sign in"
                    HorizontalAlignment="Center"
                    Margin="48,320,0,0"
                    Grid.Row="1"
                    VerticalAlignment="Top"
                    Width="306"
                    Height="45"
                    FontFamily="Segoe UI"
                    FontWeight="SemiLight"
                    Background="#FFF19229"
                    BorderBrush="{x:Null}"
                    BorderThickness="0"
                    FontSize="21"
                    Opacity="0"
                    RenderTransformOrigin="0.5,0.5"
                    Click="signUpButton_Click">
                <Button.RenderTransform>
                    <CompositeTransform />
                </Button.RenderTransform>
            </Button>
            
